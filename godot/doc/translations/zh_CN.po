"[codeblock]\n"
"set_indexed(\"position\", Vector2(42, 0))\n"
"set_indexed(\"position:y\", -10)\n"
"print(position) # (42, -10)\n"
"[/codeblock]"

#: doc/classes/Object.xml:379
msgid ""
