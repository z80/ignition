"shader_type canvas_item;\n"
"\n"
"uniform sampler2DArray tex;\n"
"uniform int index;\n"
"\n"
"void fragment() {\n"
"    COLOR = texture(tex, vec3(UV.x, UV.y, float(index)));\n"
"}\n"
