"var result = search(key, flags, line, column)\n"
"if result.size() > 0:\n"
"    # Result found.\n"
"    var res_line = result[TextEdit.SEARCH_RESULT_LINE]\n"
"    var res_column = result[TextEdit.SEARCH_RESULT_COLUMN]\n"
"[/codeblock]"
msgstr ""
