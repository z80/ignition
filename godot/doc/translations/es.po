#~ "[codeblock]\n"
#~ "const PORT = 7777\n"
#~ "var upnp = UPNP.new()\n"
#~ "upnp.discover(2000, 2, \"InternetGatewayDevice\")\n"
#~ "upnp.add_port_mapping(port)\n"
#~ "[/codeblock]\n"
